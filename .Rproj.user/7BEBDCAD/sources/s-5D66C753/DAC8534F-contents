ch_vec <- sample(x = c("John" , "Jane" ) , size = 30 , replace = TRUE , prob = c(40 , 60))
RunsTest(ch_vec)
fac_vec <- as.factor(sample(x = c("John" , "Jane" ) , size = 30 , replace = T , prob = c(40 , 60)))
RunsTest(fac_vec)
num_vec <- round(rnorm(n = 30 , mean = 100 , sd = 10 ))
RunsTest(num_vec)
